@model Student
@{ViewBag.title = "Add member";}

<div class="container col-6">
    <br />
    <h1 class="text-center">Please Enter Member Details</h1>
    <br />
    @using (Html.BeginForm(FormMethod.Post))
    {
        <div class="form-group row">
            @Html.LabelFor(w => w.Id, new { @class = "col-sm-2 col-form-label " })
            @Html.TextBoxFor(w => w.Id, new { min = "1", max = "20", placeholder = "Enter ID", @class = "col-md form-control", @type = "Number" })
        </div>
        Html.RenderPartial("CreateUpdate");

    }
</div>